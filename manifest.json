{
  "manifest_version": 2,
  "name": "TLK2_Redux",
  "description": "React, Chrome, and Redux example",
  "version": "0.0.1",
  "background": {
    "scripts": [
      "event.js"
    ],
    "persistent": true
  },
  "content_security_policy":"script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_scripts": [
    {
      "matches": ["*://app.binding-edu.org/*"],
      "css": [],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "webNavigation",
    "unlimitedStorage",
    "tabs"
  ]
}
